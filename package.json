{
	"name": "laboratorio-landing-perf-js",
	"version": "1.0.0",
	"description": "",
	"scripts": {
		"lint": "eslint ./**/*.{js,mjs,cjs,ts,mts,cts}",
		"lint:fix": "eslint --fix ./**/*.{js,mjs,cjs,ts,mts,cts}",
		"stylelint": "stylelint ./**/*.{css,scss}",
		"stylelint:fix": "stylelint --fix ./**/*.{css,scss}",
		"start": "http-server ./src -p 8080",
		"lhci": "lhci autorun --config=lighthouserc.js",
		"prepare": "husky"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"http-server": "14.1.1"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.2.2",
		"@commitlint/config-conventional": "^19.2.2",
		"@lhci/cli": "^0.13.0",
		"@stylistic/eslint-plugin": "^1.7.2",
		"@tsconfig/recommended": "^1.0.6",
		"@types/node": "^20.12.7",
		"@typescript-eslint/eslint-plugin": "^7.7.0",
		"@typescript-eslint/parser": "^7.7.0",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-check-file": "^2.8.0",
		"eslint-plugin-editorconfig": "^4.0.3",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-jsdoc": "^48.2.3",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-simple-import-sort": "^12.1.0",
		"eslint-plugin-sort-class-members": "^1.20.0",
		"eslint-plugin-tsdoc": "^0.2.17",
		"eslint-plugin-unused-imports": "^3.1.0",
		"husky": "^9.0.11",
		"prettier": "^3.2.5",
		"stylelint": "^16.3.1",
		"stylelint-config-standard": "^36.0.0",
		"stylelint-order": "^6.0.4",
		"stylelint-prettier": "^5.0.0",
		"ts-node": "^10.9.2",
		"typescript": "^5.4.5"
	},
	"imports": {
		"@/*": [
			"./*"
		],
		"@src/*": [
			"src/*"
		],
		"@tests/*": [
			"tests/*"
		],
		"@styles/*": [
			"public/*"
		],
		"@utils/*": [
			"src/utils/*"
		],
		"@templates/*": [
			"src/templates/*"
		],
		"@interface/*": [
			"src/interface/*"
		],
		"@images/*": [
			"assets/images/*"
		]
	}
}
